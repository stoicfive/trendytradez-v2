{
  "name": "@trendytradez/dashboard",
  "version": "0.1.0",
  "description": "Main dashboard package for TrendyTradez",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
